{
  "compilerOptions": {
    "target": "ES2020",
    "module": "Node16",
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "strict": true,
    "outDir": "./dist",
    "rootDir": "./",
    "sourceMap": true,
    "downlevelIteration": true,
    "lib": ["ES2020", "DOM"],
    "allowImportingTsExtensions": true,
    "noEmit": true
  },
  "include": ["frontend/**/*.ts"],
  "exclude": ["node_modules"]
}
